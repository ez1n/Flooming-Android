{
  "expo": {
    "name": "Flooming",
    "slug": "Flooming",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/flooming.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashScreen.png",
      "resizeMode": "cover"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/flooming.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.flooming.flooming",
      "versionCode": 1,
      "permissions": [
        "CAMERA"
      ]
    }
  }
}